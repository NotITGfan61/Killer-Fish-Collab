<Layer Type = "ActorFrame"><children>
	
	<Layer File = "assets/nitgfan/gold" Name = "tex_textmatcap" InitCommand="%xero(function(self) self:hidden(1) end)"/>
	
	<Layer File = "assets/plastic/sky.png" Name = "tex_sky"/>
	<Layer Type = "ActorFrame" Name = "above.frame"><children>
		<Layer File = "assets/plastic/sphere.obj" Name = "above.sky"/>
		<Layer Type = "Sprite" Frag = "assets/plastic/shaders/waterSUPERSEX.frag" Texture = "assets/plastic/stars2.jpg" Name = "above.ocean"/>
		<Layer Type = "Quad" Name = "above.oceanlight"/>
	</children></Layer>
	
	<Layer Type = "ActorFrame" Name = "under.frame"><children>
		<Layer Type = "Sprite" Frag="assets/plastic/shaders/underwatar2.frag" Texture="assets/plastic/black.jpg" Name = "under.water"/>
		<Layer Type = "Sprite" Frag="assets/plastic/shaders/sunrays.frag" Texture="assets/plastic/black.jpg" Name = "under.rays"/>
	</children></Layer>
	
	<Layer Type = "ActorFrame" Name = "itg.frame" OnCommand="%function(self) self:hidden(0)end" ><children>
		<Layer File = "assets/itg" Name = "itg.bg"/>
	</children></Layer>
	
	<Layer Type = "ActorFrame" Name = "casting.frame"><children>
		<Layer File = "assets/casting.png" Name = "casting.spellcirc"/>
		<ActorSound File="assets/spell.ogg" Name = "casting.sound"/>
	</children></Layer>
	
	<Layer Type = "ActorFrame" Name = "itgfish.frame" OnCommand="%function(self) self:hidden(1)end" ><children>
		<Layer Type = "ActorFrame" Name = "itgfish.frame2" ><children>
			<Layer File = "assets/eel.txt" Vert = "assets/fish.vert" Name = "itgfish.model"/>
		</children></Layer>
	</children></Layer>
	
	<Layer Type = "ActorFrame" Name = "apanel.frame"><children>
		<Layer Type = "Quad" Name = "apanel.bg"/>
		<Layer Type = "ActorProxy" Name = "PP_BA"/>
	</children></Layer>
	<Layer Type = "ActorFrameTexture" Name = "apanel.aft[1]" InitCommand = "%xero.aft"/>
	<Layer Type = "ActorFrameTexture" Name = "apanel.aft[2]" InitCommand = "%xero.aft"/>
	<Layer Type = "ActorFrameTexture" Name = "apanel.aft[3]" InitCommand = "%xero.aft"/>
	<Layer Type = "Quad" Name = "batman.nozbuf[1]"/>
	
	<Layer Type = "ActorFrame" Name = "bpanel.frame"><children>
		<Layer Type = "Quad" Name = "bpanel.bg"/>
		<Layer Type = "ActorProxy" Name = "PP_BB"/>
	</children></Layer>
	<Layer Type = "ActorFrameTexture" Name = "bpanel.aft[1]" InitCommand = "%xero.aft"/>
	<Layer Type = "ActorFrameTexture" Name = "bpanel.aft[2]" InitCommand = "%xero.aft"/>
	<Layer Type = "ActorFrameTexture" Name = "bpanel.aft[3]" InitCommand = "%xero.aft"/>
	<Layer Type = "Quad" Name = "batman.nozbuf[2]"/>
	
	<Layer Type  = "Quad" Name = "batman.bg"/>
	<Layer Type = "ActorFrame" Name = "batman.frame"><children>
		<Layer Type = "Polygon" Name = "batman.panelbg[1]"/>
		<Layer Type = "Polygon" Name = "batman.panelbg[2]"/>
	
		<Layer Type = "Polygon" Name = "batman.panel[1]"/>
		<Layer Type = "Polygon" Name = "batman.panel[2]"/>
	</children></Layer>
	
	<Layer Type = "ActorFrame" Name = "drop2.ppframe"><children>
		<Layer Type = "ActorProxy" Name = "PP_D2[1]"/>
		<Layer Type = "ActorProxy" Name = "PP_D2[2]"/>
		
		<Layer Type = "ActorProxy"  Name = "PP_D2plus[1]"/>
		<Layer Type = "ActorProxy"  Name = "PP_D2plus[2]"/>
	</children></Layer>
	
	<Layer Type = "ActorFrame" Name = "drop3.ppframe" OnCommand="%function(self) self:hidden(1)end" ><children>
		<Layer File = "assets/nitgfan/itgb" Name = "drop3.bg"/>
		<Layer Type = "ActorProxy" Name = "PP_D3[1]"/>
		<Layer Type = "ActorProxy" Name = "PP_D3[2]"/>
		<Layer Type = "ActorFrameTexture" Name = "drop3kecak.aft[1]" InitCommand = "%xero.aft"/>
		<Layer Type = "ActorFrameTexture" Name = "drop3kecak.aft[2]" InitCommand = "%xero.aft"/>
		<Layer Type = "ActorFrameTexture" Name = "drop3kecak.aft[3]" InitCommand = "%xero.aft"/>
		<Layer Type = "ActorFrameTexture" Name = "drop3kecak.aft[4]" InitCommand = "%xero.aft"/>
		<Layer Type = "Quad" Name = "drop3.hidequad"/>
	</children></Layer>
	
	<Layer Type = "Sprite"  Name = "drop3fade.sprite" InitCommand = "%xero.sprite" OnCommand="%function(self) self:hidden(1)end"/>
	
	<Layer Type = "ActorFrame" Name = "drop3kecak.frame" OnCommand="%function(self) self:hidden(1)end" ><children>
		<Layer Type = "Sprite" Name = "drop3kecak.sprite[1]" InitCommand = "%xero.sprite"/>
		<Layer Type = "Sprite" Name = "drop3kecak.sprite[2]" InitCommand = "%xero.sprite"/>
		<Layer Type = "Sprite" Name = "drop3kecak.sprite[3]" InitCommand = "%xero.sprite"/>
		<Layer Type = "Sprite" Name = "drop3kecak.sprite[4]" InitCommand = "%xero.sprite"/>
	</children></Layer>
	
	<Layer Type = "ActorFrameTexture" Name = "drop3fade.aft" InitCommand = "%xero.aft" OnCommand="%function(self) self:hidden(1)end"/>
	
	<Layer Type = "ActorFrameTexture" Name = "drop3scroll.aft" InitCommand = "%xero.aft"/>
	<Layer Type = "Sprite"  Frag = "assets/shaders/scroll.frag" Name = "drop3scroll.sprite" InitCommand = "%xero.sprite"/>
	
	<Layer Type = "ActorFrameTexture" Name = "drop3glitch.aft" InitCommand = "%xero.aft"/>
	<Layer Type = "Sprite"  Frag = "assets/shaders/horizon.frag" Name = "drop3glitch.sprite" InitCommand = "%xero.sprite"/>
	
	<Layer Type = "ActorFrame" Name = "drop4.ppframe"><children>
		<Layer Type = "ActorProxy" Name = "PP_D4[1]"/>
		<Layer Type = "ActorProxy" Name = "PP_D4[2]"/>
		<Layer Type = "ActorProxy" Name = "PP_D4plus"/>
	</children></Layer>
	
	<Layer Type = "ActorFrame" Name = "ending.frame" OnCommand="%function(self) self:hidden(1)end" ><children>
		<Layer File = "assets/gameoverbg" Name = "ending.bg"/>
		<Layer File = "assets/eel.txt" Name = "ending.model"/>
		<Layer File = "assets/endtext" Name = "ending.text"/>
		<Layer Type = "Quad" Name = "ending.quad"/>
	</children></Layer>
	
	<Layer Type = "ActorFrame" Name = "text.frame" OnCommand="%function(self) self:hidden(1) end" ><children>
		<Layer Frag="assets/shaders/matcap.frag" File = "assets/nitgfan/text.obj" Name = "text.model"/>
	</children></Layer>
	
	<Layer Type = "ActorProxy" Name = "PC[1]" />
	<Layer Type = "ActorProxy" Name = "PC[2]" />
	<Layer Type = "ActorProxy" Name = "PJ[1]" />
	<Layer Type = "ActorProxy" Name = "PJ[2]" />
	<Layer Type = "ActorProxy" Name = "PP[1]" />
	<Layer Type = "ActorProxy" Name = "PP[2]" />

	<Layer Type = "ActorFrameTexture" Name = "under.aft" InitCommand = "%xero.aft"/>
	<Layer Type = "Sprite" Frag = "assets/plastic/shaders/waterwibble.frag" Name = "under.aftsp" InitCommand = "%xero.sprite"/>

</children></Layer>
